{
  "logging": {
    "default": {
      "logFilePath": "/tmp/sg_logs/sg_log_rotation.log",
      "logKeys": ["*"],
      "logLevel": "debug",
      "rotation": {
        "maxsize": 1,
        "maxage": 30,
        "maxbackups": 2,
        "localtime": true
      }
    }
  },
  {{ sslcert }}
  {{ sslkey }}
  "databases": {
    "db": {
      {{ autoimport }}
      {{ xattrs }}
      {{ no_conflicts }}
      {{ sg_use_views }}
      {{ num_index_replicas }}
      {{ username1 }}
      {{ password }}
      {{ cacertpath }}
      {{ certpath }}
      {{ keypath }}
      {{ delta_sync }}
      {{ bucket_name1 }}
      "server":"{{ server_scheme }}://{{ couchbase_server_primary_node }}:{{ server_port }}"
    }
  }
}
